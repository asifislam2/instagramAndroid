<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <solid
        android:color="@android:color/transparent"
        />

    <gradient
        android:startColor="#0079DA"
        android:endColor="#DA1203"
        android:angle="135"
        />

    <stroke
        android:color="#7E5E00"
        android:width="1dp"
        />

    <size
        android:width="120dp"
        android:height="120dp"
        />
</shape>
